{
  "name": "react-bitcoin-qr",
  "version": "0.0.3",
  "description": "React component for a Bitcoin QR code. Supports optional attributes 'amount' and 'message' in addition to required attribute 'address'. Also acts as a link for browsers that support 'bitcoin:' format URIs.",
  "main": "dist.js",
  "dependencies": {
    "bitcore-lib": "^0.13.13",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-qr": "https://github.com/rBurgett/react-qr.git#update_prop_types"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "q": "^1.4.1",
    "simple-server": "^1.0.1"
  },
  "keywords": [
    "bitcoin",
    "qr",
    "address",
    "bitcore",
    "payments"
  ],
  "author": "Willy Bruns",
  "license": "MIT",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "babel index.js > dist.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willy-b/react-bitcoin-qr.git"
  },
  "bugs": {
    "url": "https://github.com/willy-b/react-bitcoin-qr/issues"
  },
  "homepage": "https://github.com/willy-b/react-bitcoin-qr#readme"
}
